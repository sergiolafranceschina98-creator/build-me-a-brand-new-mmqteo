
{
  "expo": {
    "name": "Reality Check",
    "slug": "reality-check",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/reality-check-icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/reality-check-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a2e"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.RealityCheck",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/reality-check-icon.png",
        "backgroundColor": "#1a1a2e"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.RealityCheck"
    },
    "web": {
      "favicon": "./assets/images/reality-check-icon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "reality-check",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "backendUrl": "https://p8rkpt3q9xkc754dt2csp2b3c2a6gbex.app.specular.dev"
    }
  }
}
